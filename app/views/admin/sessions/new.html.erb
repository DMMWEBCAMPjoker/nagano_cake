<div class="container">
  <div class="row">
    <table class="table">
      <h4>管理者ログイン</h4>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <tr>
        <td>メールアドレス</td>
        <td><%= f.email_field :email, autofocus: true, autocomplete: "email" %></td>
      </tr>
      <tr>
        <td>パスワード</td>
        <td><%= f.password_field :password, autocomplete: "current-password" %></td>
      </tr>
      <tr>
        <td><%= f.submit "Log in", class: "btn btn-primary" %></td>
      </tr>
      <%= render "admin/shared/links" %>
      <% end %>
    </table>
  </div>
</div>











